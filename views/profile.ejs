        <th scope="col">Fecha</th>
        <th scope="col">Total De Puntos</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <th scope="row">1</th>
        <td>Futbol</td>
        <td>5</td>
        <td>10/Noviembre</td>
        <td>5</td>
      </tr>
      <tr>
        <th scope="row">2</th>
        <td>Baloncesto</td>
        <td>4</td>
        <td>11/Noviembre</td>
        <td>9</td>
      </tr>
      <tr>
        <th scope="row">3</th>
        <td>Caminata</td>
        <td>10</td>
        <td>15/Noviembre</td>
        <td>19</td>
      </tr>
      <tr>
        <th colspan="5">%</th>          
      </tr>
    </tbody>
</table>
 
 <div class="progress">
